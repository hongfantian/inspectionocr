<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width = "26dp"
                android:height = "26dp" />
            <stroke
                android:width="1dp"
                android:color="@color/black" />
            <solid android:color="@color/white" />
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"
        android:drawable="@drawable/ic_view_check"
        android:width="20dp"
        android:height="20dp"></item>
</layer-list>